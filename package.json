{
  "name": "ts_env_for_lha",
  "version": "1.0.0",
  "description": "ts env for node scripts with jest and esbuild",
  "main": "src/app.ts",
  "scripts": {
    "start": "concurrently \"bash run_dev_server.sh\" \"nodemon\"",
    "test": "jest --watchAll",
    "build": "npx esbuild src/tasks/module4/dynamicTable/dynamicTable.ts --bundle --minify --outfile=build/dynamicTable/dynamicTable.js --watch"
  },
  "keywords": [
    "env",
    "typescript",
    "simple",
    "tasks",
    "node",
    "esbuild",
    "jest"
  ],
  "license": "ISC",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/is": "^0.0.21",
    "@types/nodemailer": "^6.4.0",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "is_js": "^0.9.0",
    "nodemailer": "^6.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "concurrently": "^6.0.0",
    "esbuild": "^0.8.56",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  }
